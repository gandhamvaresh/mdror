					<span style="<%= 'text-decoration:underline; font-weight:bold;' if @chiropractic_examination.chiropractic_symptom_list.neck_pain_radiates_to_digit1? %>">1</span>&nbsp;
					<span style="<%= 'text-decoration:underline; font-weight:bold;' if @chiropractic_examination.chiropractic_symptom_list.neck_pain_radiates_to_digit2? %>">2</span>&nbsp;
					<span style="<%= 'text-decoration:underline; font-weight:bold;' if @chiropractic_examination.chiropractic_symptom_list.neck_pain_radiates_to_digit3? %>">3</span>&nbsp;
					<span style="<%= 'text-decoration:underline; font-weight:bold;' if @chiropractic_examination.chiropractic_symptom_list.neck_pain_radiates_to_digit4? %>">4</span>&nbsp;
					<span style="<%= 'text-decoration:underline; font-weight:bold;' if @chiropractic_examination.chiropractic_symptom_list.neck_pain_radiates_to_digit5? %>">5</span></td>
			</tr>
			
			<tr>
				<td><%= check_box_for_display(:upper_mid_back_pain, @chiropractic_examination.chiropractic_symptom_list.upper_mid_back_pain?, 'Upper/Mid-back Pain') %></td>
