<div class="span-24">
	<center><%= image_tag @client.report_header.url(:banner) if @client.report_header.exists? %> <center>
</div>
	
<% if mobile_device? %>
	<div class="prepend-21 span-3">
		<%= link_to image_tag('back.png'), patient_accident_chiropractic_examination_path(@patient, @accident, @chiropractic_examination), :class => "non_printable_block" %> 
		&nbsp; &nbsp; &nbsp;
		<a href="#" onclick="window.print();return false;" class="non_printable_block">
			<%= image_tag('printer.png') %> 
		</a>
	</div>
<% else %>
	<div class="prepend-22 span-2">
		<a href="#" onclick="window.print();return false;" class="non_printable_block">
			<%= image_tag('printer.png') %> 
		</a>
	</div>
<% end %>

<hr class="space"/>

<h3 style="text-align:center; text-transform: uppercase; text-decoration:underline;">Initial Chiropractic Examination</h3>

<hr class="space"/>

<div class="span-24">
	<div class="span-2">
		<label>HISTORY:</label>
	</div>
	
	<div class="span-2">
		<span>Last Name:</span>
	</div>
	
	<div class="span-3 underlined">
		<%= @patient.last_name %>
	</div>
	
	<div class="span-2">
		<span>First Name:</span>
	</div>
	
	<div class="span-4 underlined">
		<%= @patient.first_name %>
	</div>
	
	<div class="span-1">
		<span>Age:</span>
	</div>
	
	<div class="span-2 underlined">
		<%= @patient.age %> yrs
	</div>
	
	<div class="span-1">
		<span>Sex:</span>
	</div>
	
	<div class="span-2 underlined">
		<%= @patient.gender %>
	</div>
	
	<div class="span-3">
		<span>Date of Exam:</span>
	</div>
	
	<div class="span-2 underlined last">
		<%= @chiropractic_examination.examination_date.strftime('%b/%d/%Y') %>
	</div>
</div>

<hr class="space"/>

<div class="span-24">
	<div class="span-2">
		<label>Occupation:</label>
	</div>
	
	<div class="span-5 underlined">
		<%= @patient.occupation %>
	</div>
	
	<div class="span-6">
		<span>lost time from work?</span>&nbsp;
		<%= check_box_for_display(:lost_time_from_work, @accident.lost_time_from_work?, 'Y') %>
		<%= check_box_for_display(:lost_time_from_work, !@accident.lost_time_from_work?, 'N') %>
	</div>
	
	<div class="span-4">
		<span style="<%= 'text-decoration:underline; font-weight:bold;' if @patient.handed == 'Left' %>">Left</span>&nbsp;
		<span style="<%= 'text-decoration:underline; font-weight:bold;' if @patient.handed == 'Right' %>">Right</span>&nbsp;
		&nbsp;&nbsp;Handed
	</div>
	
	<div class="span-1">
		<span>Height</span>
	</div>
	
	<div class="span-3 underlined">
		<%= @patient.height %>
	</div>
	
	<div class="span-1">
		<span>Weight</span>
	</div>
	
	<div class="span-2 underlined last">
	<%= @patient.weight %> Pounds
	</div>
</div>

<hr class="space"/>

<div class="span-24">
	
	<div class="span-2">
		<%= check_box_for_display(:mva, @accident.accident_type == 'MVA', 'MVA') %>
	</div>
	
	<div class="span-3">
		<%= check_box_for_display(:slip_and_fall, @accident.accident_type == 'Slip and Fall', 'Slip and Fall') %>
	</div>
	
	<div class="span-3">
		<%= check_box_for_display(:work_related, @accident.accident_type == 'Work Related', 'Work Related') %>
	</div>
	
	<div class="span-2">
		Injury Date
	</div>
	
	<div class="span-2 underlined">
		<%= @accident.injury_date.strftime('%b/%d/%Y') %>
	</div>
	
	<div class="span-4">
		<%= check_box_for_display(:wearing_seat_belt, @accident.wearing_seat_belt?, 'Wearing Seatbelt') %>
	</div>
	
	<div class="span-3">
		<%= check_box_for_display(:pedestrian, @accident.pedestrian?, 'Pedestrian') %>
	</div>
	
	<div class="span-2 last">
		<%= check_box_for_display(:driver, @accident.driver?, 'Driver') %>
	</div>
	
	<div class="span-3 last">
		<%= check_box_for_display(:passenger, @accident.passenger?, 'Passenger') %>
	</div>
</div>

<hr class="space"/>

<div class="span-24">
	<div class="span-2">
		<%= check_box_for_display(:front, @accident.seating_position =~ /Front/, 'Front') %>
	</div>
	
	<div class="span-2">
		<%= check_box_for_display(:rear, @accident.seating_position =~ /Rear/, 'Rear') %>
	</div>
	
	<div class="span-4">
		<%= check_box_for_display(:passenger_side, @accident.seating_position =~ /Passenger/, 'Passenger Side') %>
	</div>
	
	<div class="span-2">
		<%= check_box_for_display(:middle, @accident.seating_position =~ /Middle/, 'Middle') %>
	</div>
	
	<div class="span-3">
		<%= check_box_for_display(:driver_side, @accident.seating_position =~ /Driver/, "Driver's Side") %>
	</div>
	
	<div class="span-5">
		Airbag Deploy:&nbsp;
		<%= check_box_for_display(:airbag_deploy, @accident.airbag_deploy?, "Y") %>
		<%= check_box_for_display(:airbag_deploy, !@accident.airbag_deploy?, "N") %>
	</div>
	
	<div class="span-5 last">
		LOC:&nbsp;
		<%= check_box_for_display(:loss_of_consciousness, @accident.loss_of_consciousness?, "Y") %>
	  <%= check_box_for_display(:loss_of_consciousness, !@accident.loss_of_consciousness?, "N") %>
	</div>
</div>

<hr class="space"/>

<div class="span-24">
	<div class="span-4">Description of Injury:</div>
	<div class="span-20 underlined last">
		<% if @accident.injury_description.blank? %>
			&nbsp;
		<% else %>
			<%= @accident.injury_description %>
		<% end %>
	</div>
</div>

<hr class="space"/>

<div class="span-24">
	<div class="span-4">
		Hospital:&nbsp;
		<%= check_box_for_display(:taken_to_hospital, @accident.taken_to_hospital?, "Y") %>
		<%= check_box_for_display(:taken_to_hospital, !@accident.taken_to_hospital?, "N") %>&nbsp;&nbsp;&nbsp;by&nbsp;
	</div>
	
	<div class="span-3">
		<%= check_box_for_display(:ambulance, @accident.hospital_transport_mode == "Ambulance", "Ambulance") %>
	</div>
	
	<div class="span-2">
		<%= check_box_for_display(:car, @accident.hospital_transport_mode == "Car", "Car") %>
	</div>
	
	<div class="span-3">
		<%= check_box_for_display(:examined, @accident.examined?, "Examined") %>
	</div>
	
	<div class="span-2">
		<%= check_box_for_display(:xrays, @accident.xrays?, "X-rays") %>
	</div>
	
	<div class="span-2">
		<%= check_box_for_display(:ctscan, @accident.ctscan?, "CT") %>
	</div>
	
	<div class="span-2">
		<%= check_box_for_display(:mri, @accident.mri?, "MRI") %>
	</div>
	
	<div class="span-3">
		<%= check_box_for_display(:released, @accident.released?, "Released") %>
	</div>
	
	<div class="span-3 last">
		<%= check_box_for_display(:admitted, @accident.admitted?, "Admitted") %>
	</div>
</div>

<% unless @accident.hospital_name.blank? %>
	<hr class="space"/>
	
	<div class="span-24">
		<div class="span-4">Hospital Name:</div>
		<div class="span-20 underlined last">
			<%= @accident.hospital_name %>
		</div>
	</div>

<% end %>

<hr class="space"/>

<div class="span-24">
	<div class="span-4">Medications:</div>
	<div class="span-20 underlined last">
		<% if @accident.medications.blank? %>
			&nbsp;
		<% else %>
			<%= @accident.medications %>
		<% end %>
	</div>
</div>

<hr class="space"/>

<div class="span-24">
	<div class="span-4">Prior Injuries:</div>
	<div class="span-20 underlined last">
		<% if @accident.prior_injuries.blank? %>
			&nbsp;
		<% else %>
			<%= @accident.prior_injuries %>
		<% end %>
	</div>
</div>

<hr class="space"/>

<div class="span-24">
	<div class="span-4">
		Prior Diagnostic Studies:
	</div>
	
	<div class="span-4">
		<%= check_box_for_display(:prior_diagnostic_study_mri, !@accident.prior_diagnostic_study_mri.blank?, "MRI:") %>
		<span style="<%= 'text-decoration:underline; font-weight:bold;' if @accident.prior_diagnostic_study_mri == 'Cervical' %>">C</span>&nbsp;
		<span style="<%= 'text-decoration:underline; font-weight:bold;' if @accident.prior_diagnostic_study_mri == 'Lumbar' %>">L</span>&nbsp;Spine
	</div>
	
	<div class="span-1">Other:</div>
	<div class="span-3 underlined last">
		<% if @accident.prior_diagnostic_study_mri_other.blank? %>
			&nbsp;
		<% else %>
			<%= @accident.prior_diagnostic_study_mri_other %>
		<% end %>
	</div>
	
	<div class="span-4">
		<%= check_box_for_display(:prior_diagnostic_study_ctscan, !@accident.prior_diagnostic_study_ctscan.blank?, "CT Scan:") %>
		<span style="<%= 'text-decoration:underline; font-weight:bold;' if @accident.prior_diagnostic_study_ctscan == 'Cervical' %>">C</span>&nbsp;
		<span style="<%= 'text-decoration:underline; font-weight:bold;' if @accident.prior_diagnostic_study_ctscan == 'Lumbar' %>">L</span>&nbsp;Spine
	</div>
	
	<div class="span-1">Other:</div>
	<div class="span-3 underlined last">
		<% if @accident.prior_diagnostic_study_ctscan_other.blank? %>
			&nbsp;
		<% else %>
			<%= @accident.prior_diagnostic_study_ctscan_other %>
		<% end %>
	</div>
	
	<div class="span-4 last">
		<%= check_box_for_display(:prior_diagnostic_study_emg, !@accident.prior_diagnostic_study_emg.blank?, "EMG:") %>
		<span style="<%= 'text-decoration:underline; font-weight:bold;' if @accident.prior_diagnostic_study_emg == 'Upper' %>">Upper</span>/<span style="<%= 'text-decoration:underline; font-weight:bold;' if @accident.prior_diagnostic_study_emg == 'Lower' %>">Lower</span>
	</div>
</div>

<hr class="space"/>

<div class="span-24">
	<div class="span-4">Results:</div>
	<div class="span-20 underlined last">
		<% if @accident.prior_diagnostic_results.blank? %>
			&nbsp;
		<% else %>
			<%= @accident.prior_diagnostic_results %>
		<% end %>
	</div>
</div>

<hr class="space"/>

<div class="span-24">
	<div class="span-4">Medical History:</div>
	<div class="span-20 underlined last">
		<% if @accident.medical_history.blank? %>
			&nbsp;
		<% else %>
			<%= @accident.medical_history %>
		<% end %>
	</div>
</div>

<hr class="space"/>

<div class="span-24">
	<div class="span-16">
		<br/>
		<%= check_box_for_display(:significant_prior_illness_or_injury, @chiropractic_examination.chiropractic_symptom_list.significant_prior_illness_or_injury?, "No significant prior illness / injury / medical condition.") %>
	</div>
	
	<div class="span-8 last">
		<table style="border:thin solid #000000;">
			<tbody>
				<tr>
					<td style="border-right:thin solid #000000;" rowspan="2">Pain Level on VAS</td> 
					<td style="border-right:thin solid #000000;">Resting</td>
					<td><%= @chiropractic_examination.chiropractic_symptom_list.pain_level_on_vas_resting%>/10</td>
				</tr>
				<tr>
					<td style="border-top:thin solid #000000;">Activity</td>
					<td style="border-left:thin solid #000000; border-top:thin solid #000000;"><%= @chiropractic_examination.chiropractic_symptom_list.pain_level_on_vas_activity%>/10</td>
				</tr>
			</tbody>
		</table>
	</div>
</div>

<div class="span-24">
	<table>
		<thead>
			<th class="span-4">SYMPTOMS</th>
			<th class="span-4">&nbsp;</th>
			<th class="span-3">Scale 1 - 10</th>
			<th class="span-2">&nbsp;</th>
			<th class="span-11 last">Radiates</th>
		</thead>
		
		<tbody>
			<tr>
				<td><%= check_box_for_display(:neck_pain, @chiropractic_examination.chiropractic_symptom_list.neck_pain?, 'Neck Pain') %></td>
				<td> 
					<span style="<%= 'text-decoration:underline; font-weight:bold;' if @chiropractic_examination.chiropractic_symptom_list.neck_pain_side =~ /Right/ %>">R</span>/
					<span style="<%= 'text-decoration:underline; font-weight:bold;' if @chiropractic_examination.chiropractic_symptom_list.neck_pain_side =~ /Left/ %>">L</span>/
					<span style="<%= 'text-decoration:underline; font-weight:bold;' if @chiropractic_examination.chiropractic_symptom_list.neck_pain_side =~ /Both/ %>">B</span>
				</td>
				<td> 
					<% if @chiropractic_examination.chiropractic_symptom_list.neck_pain? %> 
							<%= @chiropractic_examination.chiropractic_symptom_list.neck_pain_scale %>
					<% else %>
						&nbsp;
					<% end %>
				</td>
				<td>&nbsp;</td>
				<td>
					<span style="<%= 'text-decoration:underline; font-weight:bold;' if @chiropractic_examination.chiropractic_symptom_list.neck_pain_radiation_side =~ /Right/ %>">R</span>/
					<span style="<%= 'text-decoration:underline; font-weight:bold;' if @chiropractic_examination.chiropractic_symptom_list.neck_pain_radiation_side =~ /Left/ %>">L</span>/
					<span style="<%= 'text-decoration:underline; font-weight:bold;' if @chiropractic_examination.chiropractic_symptom_list.neck_pain_radiation_side =~ /Both/ %>">B</span> 
					<%= check_box_for_display(:neck_pain_radiates_to_shoulder, @chiropractic_examination.chiropractic_symptom_list.neck_pain_radiates_to_shoulder?, 'To Shoulder') %>&nbsp;
					<%= check_box_for_display(:neck_pain_radiates_to_elbow, @chiropractic_examination.chiropractic_symptom_list.neck_pain_radiates_to_elbow?, 'To Elbow') %>&nbsp;
					<%= check_box_for_display(:neck_pain_radiates_to_hand, @chiropractic_examination.chiropractic_symptom_list.neck_pain_radiates_to_hand?, 'Hand') %>&nbsp;
