<% unless disable_option %>
	<div class="box-white">
			<p>
				<%= f.label :treatment_date %>
			</p>

			<p>
				&nbsp;&nbsp;&nbsp;&nbsp;<%= f.label :month %>
				<%= select_month(@chiropractic_treatment.treatment_date.try(:month) || Date.today, :add_month_numbers => true, :prefix => 'chiropractic_treatment', :field_name => 'treatment_date(2i)', :disabled => disable_option) %>
			</p>

			<p>
				&nbsp;&nbsp;&nbsp;&nbsp;<%= f.label :day %>
				<%= select_day(@chiropractic_treatment.treatment_date.try(:day) || Date.today, :prefix => 'chiropractic_treatment', :field_name => 'treatment_date(3i)', :disabled => disable_option) %>
			</p>

			<p>
				&nbsp;&nbsp;&nbsp;&nbsp;<%= f.label :year %>
				<%= select_year(@chiropractic_treatment.treatment_date.try(:year) || Date.today, :start_year => Date.today.year - 100, :end_year => Date.today.year, :prefix => 'chiropractic_treatment', :field_name => 'treatment_date(1i)', :disabled => disable_option) %>
			</p>
	</div>
	<br/>
<% end %>

